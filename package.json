{
    "name": "beading-studio",
    "version": "1.0.0",
    "description": "Редактор схем для бисероплетения",
    "main": "electron-main.js",
    "scripts": {
        "start": "electron .",
        "build": "electron-packager . BeadingPatternEditor --platform=win32 --arch=x64 --overwrite --out=dist --no-prune",
        "build-all": "electron-packager . BeadingPatternEditor --platform=all --arch=x64 --overwrite --out=dist"
    },
    "keywords": [
        "beading",
        "pattern",
        "editor",
        "electron"
    ],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "electron": "^28.0.0",
        "electron-packager": "^17.1.2"
    }
}